{
  "format": "custom",
  "id": "events",
  "label": "Events",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "image": {
        "type": "Image",
        "config": {
          "label": "Imagen",
          "constraint": {},
          "thumbnails": []
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Título",
          "placeholder": ""
        }
      },
      "descripcion": {
        "type": "StructuredText",
        "config": {
          "label": "Descripción",
          "placeholder": "Descripción del evento",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "day": {
        "type": "Date",
        "config": {
          "label": "Día",
          "placeholder": "Dia del evento"
        }
      },
      "hour": {
        "type": "Text",
        "config": {
          "label": "Hora",
          "placeholder": "Horario del evento"
        }
      },
      "hour_end": {
        "type": "Text",
        "config": {
          "label": "Hora de fin",
          "placeholder": "Horario de finalización (opcional)"
        }
      },
      "categoria": {
        "type": "Text",
        "config": {
          "label": "Categoría",
          "placeholder": "Categoría del evento"
        }
      },
      "more": {
        "type": "Link",
        "config": {
          "label": "Mas Info",
          "placeholder": "Más info (Link)",
          "select": null,
          "allowTargetBlank": true,
          "allowText": true,
          "repeat": false
        }
      }
    }
  }
}
